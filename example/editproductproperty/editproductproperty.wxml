<!--example/editproductproperty/editproductproperty.wxml-->
<view class="page">
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell"  >
          
                <view class="weui-cell__bd">{{property.title}}</view>
                <view class="weui-cell__ft">
                  <input class="weui-input" placeholder="" value="{{property.value}}"/>
                </view>
            </view>
        </view>
        


        <view class="weui-btn-area" >
           <button class="weui-btn" type="primary" bindtap="bindSaveTap">保存</button>
        </view>
    </view>
</view>
